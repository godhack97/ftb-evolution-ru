	quest.50E0C5ECFB5B02E1.title: "&7Foliot Otherstone Trader"
	quest.50EFBD8BDE0C88AF.quest_desc: ["&aОбновление &rдиапазона &3увеличивает &rдиапазон беспроводного передатчика, с 16 до 48 блоков."]
	quest.50EFBD8BDE0C88AF.title: "&aОбновление диапазона"
	quest.50F2EDF92D289869.quest_desc: [
